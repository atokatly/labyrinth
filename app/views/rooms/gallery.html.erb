<style>
body,h1,h2,h3,h4,h5,h6 {font-family: "Lato", sans-serif;}
body, html {
    height: 92.5%;
    color: #777;
    line-height: 1.8;
}
footer {
  background-color: #0A5CAD;
  color: #ffffff;
}
/* Create a Parallax Effect */
.bgimg-1, .bgimg-2, .bgimg-3 {
    background-attachment: fixed;
    background-position: 5% 60%;
    background-repeat: no-repeat;
    background-size: cover;
}

.left {
  background-position: left;
}

.right {
  background-position: right;
}

.center {
  background-position: center;
}

/* First image (Logo. Full height) */
.bgimg-1 {
    background-image: url(<%= asset_path "aztec-resize.jpg", alt: "Tokyo Logo" %>);
    min-height: 100%;
}

.w3-wide {letter-spacing: 10px;}
.w3-hover-opacity {cursor: pointer;}

/* Turn off parallax scrolling for tablets and phones */
@media only screen and (max-device-width: 1024px) {
    .bgimg-1, .bgimg-2, .bgimg-3 {
        background-attachment: scroll;
    }
    .w3-third {
      width: 32%;
    }

    .w3-quarter {
      width: 24.75%;
    }

    .w3-large {
      font-size: 15px !important;
    }
}
/*styles for landscape mode on mobile devices*/
@media only screen and (orientation:landscape)
and (max-device-width: 1024px){
  .bgimg-1, .bgimg-2, .bgimg-3, #showcase {
      background-attachment: scroll;
  }
  .w3-bar-item{
    width: 24%;
  }
}
@media only screen and (orientation:portrait)
and (max-device-width: 1024px){
  .bgimg-1{
    background-image: url(<%= asset_path "sneak-peek-aztec.jpg", alt: "Tokyo Logo" %>);
    background-position: center;
    min-height: 400px;
  }
  body{
    height: 40%;
  }
  .gallery{
    width: 100% !important;
    height: 100%;
  }
}
div#bookeo_position {
  background-color: #777;
  margin: 0 auto;
  display: block;
  text-align: center !important;;
  font-family: "Lato", sans-serif;
  border: solid 2px rgba(0, 0, 0, 0.4);
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
  -webkit-box-shadow: 0 0 9px 1px rgba(0, 0, 0, 0.4);
  -moz-box-shadow: 0 0 9px 1px rgba(0, 0, 0, 0.4);
  box-shadow: 0 0 9px 1px rgba(0, 0, 0, 0.4);
  background-color: Transparent;
  background-repeat:no-repeat;
  border: none;
  cursor:pointer;
  overflow: hidden;
  outline:none;
  min-width: 10%;
  width: auto !important;;
  margin-top: 10px;
  height: auto !important;;

       }
        div#bookeoStartMobile{
         width: auto !important;
         padding-bottom: 35px !important;
       }
       div#bookeo:hover{
         background-color: #FF2600 !important;
       }
       .w3-show{
         position: fixed;
         z-index: 100;
         /*border: 1px solid black;*/
       }
       .navBar {
         z-index: 101;
       }
  .gallery{
    width: 49%;
    padding-top: 8px;
  }
</style>
