  text-align: center;
  margin-top: 10px;
  margin-bottom: 10px;
}
#nav {
  text-align: center;
}
#nav > button {
  width: 64px;
  height: 36px;
  color: #666;
  font: bold 16px arial;
  text-align: center;
  margin: 5px;
  text-shadow: 0px 1px 0px #f5f5f5;
  background: #f6f6f6;
